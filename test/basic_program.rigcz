class Vector3
{
	x : Int32;
	y : Int32;
	z : Int32;

	construct {
		self.x = 10;
		self.y = 20;
		self.z = 30;
		print("typeOf(self) = {}\n", typeOf(self));
	}

	sum {
		ret self.x + self.y + self.z;
	}
}

func sumOf(v : Vector3) {
	ret v.x + v.y + v.z;
}

func main {
	Vector3 pos;
	pos.construct();
	print("Pos: ({}, {}, {}), sum = {}\n", pos.x, pos.y, pos.z, pos.sum());
}
